DMU = c("Facility 1", "Facility 2", "Facility 3", "Facility 4", "Facility 5", "Facility 6"),
CRS_Peers = sapply(1:6, function(i) get_peer_string(peers(efficiency_crs), i)),
IRS_Peers = sapply(1:6, function(i) get_peer_string(peers(efficiency_irs), i)),
DRS_Peers = sapply(1:6, function(i) get_peer_string(peers(efficiency_drs), i)),
VRS_Peers = sapply(1:6, function(i) get_peer_string(peers(efficiency_vrs), i)),
FDH_Peers = c("Self", "Self", "Self", "Self", "Self", "Self"),
FRH_Peers = sapply(1:6, function(i) get_peer_string(peers(efficiency_frh), i))
)
kable(peer_summary,
caption = "Summary of Peer Units Across All DEA Models",
booktabs = TRUE) %>%
kable_styling(bootstrap_options = c("striped", "bordered", "hover"),
full_width = TRUE) %>%
row_spec(0, bold = TRUE, background = "#D3D3D3") %>%
column_spec(1, bold = TRUE)
