# Function to get peer string
get_peer_string <- function(peers_obj, index) {
peer_vec <- peers_obj[index, ]
peer_vec <- peer_vec[!is.na(peer_vec) & peer_vec != 0]
if (length(peer_vec) == 0 || (length(peer_vec) == 1 && peer_vec[1] == index)) {
return("Self")
}
return(paste(peer_vec, collapse = ", "))
}
